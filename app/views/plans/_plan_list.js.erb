var deletePlan = function(e) {
  var id = e.parentNode.parentNode.id;
  var data = {id: id, project_id: '<%= params[:project_id]%>'};
  $.ajax({
    type: 'DELETE',
    dataType: 'JSON',
    url: 'plans/'+id+'?project_id=<%= params[:project_id] %>', 
    data: data,
    success: function() {
      $('#'+id).hide()
    }
    });    
}
